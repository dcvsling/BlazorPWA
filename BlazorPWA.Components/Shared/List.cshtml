@typeparam T
@inherits ComponentBase


@foreach (var item in Source)
{
    @Item(item)
}

@functions {
    [Parameter] RenderFragment<T> Item { get; set; }
    [Parameter] IReadOnlyList<T> Source { get; set; }
    [Parameter] string ULClass { get; set; }
    [Parameter] string LIClass { get; set; }
}